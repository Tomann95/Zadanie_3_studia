using System;
using System.Linq;

class Program
{
    static void Main()
    {
        int[] tablica = { 5, 2, 8, 3, 1, 9, 4, 7, 6 };

        int najmniejszy = tablica.Min();
        int największy = tablica.Max();

        Console.WriteLine("Najmniejszy element: " + najmniejszy);
        Console.WriteLine("Największy element: " + największy);

        Console.ReadLine();
    }
}


